	border: 1px solid #AB9888; /* @button_border */
	border-radius: 3px;
	background:
		#E6DDD5 /* @theme_bg */
		qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
			stop:0       /* @button_glaze_top */ rgba(248,246,243,0.17),
			stop:0.49   /* @button_glaze_mida */ rgba(235,228,221,0.80),
			stop:0.4901 /* @button_glaze_midb */ rgba(218,205,194,0.90),
			stop:1       /* @button_glaze_bot */ rgba(221,210,200,0.90));
}

QPushButton:checked {
