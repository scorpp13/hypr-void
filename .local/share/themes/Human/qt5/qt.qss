QWidget {
	color: #101010; /* @theme_fg */
	selection-color: #FFFFFF; /* @theme_selected_fg */
	selection-background-color: #8F5F4A; /* @theme_selected_bg */
}

QMainWindow, QWindow, QDialog {
	background-color: #E6DDD5; /* @theme_bg */
}

QGroupBox {
	color: #101010; /* @theme_fg */
	padding: 18px 0 0;
}

QGroupBox::title {
	padding: 0 3px;
}

QToolTip {
	padding: 1px 2px;
	color: #000000; /* @theme_tooltip_fg */
	border: 1px solid #C0C04C; /* @theme_tooltip_border */
	background-color: #F5F5B5; /* @theme_tooltip_bg */
}

:disabled {
	color: #A28F7E; /* @insensitive_fg */
}

/* --- menu & menu items --- */

QMenuBar {
	spacing: 0;
	padding: 1px 1px 0;
	border-bottom: 1px solid #C9B9AB; /* @menubar_border_bot */
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		stop:0 /* @menubar_grad_top */ #E6DDD5,
		stop:1 /* @menubar_grad_bot */ #DED2C8);
}

QMenuBar::item {
	padding: 2px 7px 2px;
	border: 1px solid transparent;
	/* border-radius: 2px 2px 0 0; @todo not working */
}

QMenuBar::item:pressed {
	color: #FFFFFF; /* @menuitem_hover */
	border-color: #684B3F; /* @menuitem_hover_border */
	border-bottom-color: #885C48; /* @menuitem_hover_border_bottom */
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		stop:0       /* @menuitem_hover_glaze_top */ #97624C,
		stop:0.5    /* @menuitem_hover_glaze_mida */ #885C48,
		stop:0.5001 /* @menuitem_hover_glaze_midb */ #885C48,
		stop:1       /* @menuitem_hover_glaze_bot */ #885C48);
}

QMenu {
	padding: 2px;
	border: 1px solid #B4A291; /* @menu_border */
	background-color: #EEE8E3; /* @menu_bg */
}

QMenu::item {
	padding: 2px 4px 2px 6px;
	border: 1px solid transparent;
	border-radius: 2px;
}

QMenu::tearoff:selected,
QMenu::item:selected {
	color: #FFFFFF; /* @menuitem_hover */
	border-color: #684B3F; /* @menuitem_hover_border */
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		stop:0       /* @menuitem_hover_glaze_top */ #97624C,
		stop:0.5    /* @menuitem_hover_glaze_mida */ #885C48,
		stop:0.5001 /* @menuitem_hover_glaze_midb */ #885C48,
		stop:1       /* @menuitem_hover_glaze_bot */ #885C48);
}

QMenu::icon {
	margin: 0 0 0 10px;
}

QMenu::separator {
	margin: 4px 0 3px;
	height: 0;
	border-top: 1px solid rgba(187,170,154,0.50); /* @menu_separator_dark */
	border-bottom: 1px solid rgba(255,255,255,0.50); /* @menu_separator_lite */
}

QMenu::right-arrow {
	width: 19px;
	height: 19px;
	image: url('/usr/share/icons/Adwaita/symbolic/ui/pan-end-symbolic.svg');
}

QMenu::left-arrow {
	width: 19px;
	height: 19px;
	image: url('/usr/share/icons/Adwaita/symbolic/ui/pan-end-symbolic-rtl.svg');
}

QMenu::right-arrow:selected {
	/* @todo recolor */
}

QMenu::left-arrow:selected {
	/* @todo recolor */
}

/* --- check & radio menu items --- */

QMenu::indicator {
	width: 16px;
	height: 16px;
	margin: 1px -1px 0 5px;
}

QMenu::indicator:checked {
	image: url("img/menu-checkbox-checked.svg");
}

QMenu::indicator:checked:selected {
	image: url("img/menu-checkbox-checked-hover.svg");
}

QMenu::indicator:checked:exclusive {
	image: url("img/menu-radio-checked.svg");
}

QMenu::indicator:checked:exclusive:selected {
	image: url("img/menu-radio-checked-hover.svg");
}

QMenu::indicator:checked:disabled {
	image: url("img/menu-checkbox-checked-insensitive.svg");
}

QMenu::indicator:checked:exclusive:disabled {
	image: url("img/menu-radio-checked-insensitive.svg");
}

/* --- tool bar --- */

QToolBar {
	spacing: 0;
	padding: 3px;
	border-top: 1px solid #F0EAE5; /* @toolbar_border_top */
	border-bottom: 1px solid #C9B9AB; /* @toolbar_border_bot */
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		stop:0 /* @toolbar_grad_top */ #E6DDD5,
		stop:1 /* @toolbar_grad_bot */ #DDD2C8);
}

QToolBar::separator {
	width: 0;
	margin: 6px 6px 7px 4px;
	border-left: 1px solid rgba(179,161,144,0.50); /* @toolbar_separator_dark */
	border-right: 1px solid rgba(255,255,255,0.50); /* @toolbar_separator_lite */
}

QToolBar QToolButton {
	margin: 0 2px 0 0;
	padding: 3px 2px 2px 3px;
	border: 1px solid transparent;
	border-radius: 3px;
}

QToolButton:open {
	padding: 4px 1px 1px 4px;
}

QToolButton:hover {
	border-color: #AB9888; /* @button_border */
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		stop:0       /* @button_hover_glaze_top */ rgba(245,241,238,0.80),
		stop:0.5    /* @button_hover_glaze_mida */ rgba(239,232,227,0.80),
		stop:0.5001 /* @button_hover_glaze_midb */ rgba(223,212,202,0.80),
		stop:1       /* @button_hover_glaze_bot */ rgba(218,205,194,0.80));
}

QToolButton:pressed,
QToolButton:checked {
	border-color: #AB9888; /* @button_border */
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		stop:0       /* @button_active_glaze_top */ rgba(210,196,184,0.78),
		stop:0.49   /* @button_active_glaze_mida */ rgba(197,181,166,0.80),
		stop:0.4901 /* @button_active_glaze_midb */ rgba(185,167,152,0.80),
		stop:1       /* @button_active_glaze_bot */ rgba(193,176,161,0.79));
}

/* --- tool bar item with menu --- */

QToolButton::menu-arrow {
	width: 16px;
	height: 16px;
	margin: 0 1px 0 -1px;
	image: url('/usr/share/icons/Adwaita/symbolic/ui/pan-down-symbolic.svg');
}

QToolButton::menu-arrow:open {
	margin: 1px 0 -1px -1px;
}

QToolButton::menu-arrow:disabled {
	/* @todo recolor */
}

QToolButton[hasMenu="true"],
[textDirection="ltr"] QToolButton[hasMenu="true"] { /* @todo */
	width: 24px;
	padding-right: 29px;
}

[textDirection="rtl"] QToolButton[hasMenu="true"] { /* @todo */
	width: 24px;
	padding-left: 29px;
}

QToolButton[hasMenu="true"]::menu-button {
	width: 24px;
	border-left: 1px solid transparent;
	background: transparent;
}

QToolButton[hasMenu="true"]::menu-button:hover,
QToolButton[hasMenu="true"]::menu-button:active {
	border-left-color: #AB9888; /* @button_border */
}

/* --- status bar --- */

QStatusBar {
	max-height: 20px; /* @todo */
	border-top: 1px solid #C9B9AB; /* @toolbar_border_bot */
	background-color: #E6DDD5; /* @theme_bg */
}

/* --- checkbox --- */

QCheckBox, QRadioButton {
	spacing: 0;
	margin: 0 0 1px 1px;
	padding: 1px 0 2px;
}

QTreeView::indicator,
QCheckBox::indicator {
	width: 16px;
	height: 16px;
	margin: 1px 5px 0 0;
	image: url("img/checkbox-unchecked.svg");
}

QTreeView::indicator:disabled,
QCheckBox::indicator:disabled {
	image: url("img/checkbox-unchecked-insensitive.svg");
}

QTreeView::indicator:checked,
QCheckBox::indicator:checked {
	image: url("img/checkbox-checked.svg");
}

QTreeView::indicator:checked:selected,
QCheckBox::indicator:checked:pressed {
	image: url("img/checkbox-checked-active.svg");
}

QTreeView::indicator:checked:disabled,
QCheckBox::indicator:checked:disabled {
	image: url("img/checkbox-checked-insensitive.svg");
}

QTreeView::indicator:indeterminate,
QCheckBox::indicator:indeterminate {
	image: url("img/checkbox-mixed.svg");
}

QTreeView::indicator:indeterminate:selected,
QCheckBox::indicator:indeterminate:pressed {
	image: url("img/checkbox-mixed-active.svg");
}

QTreeView::indicator:indeterminate:disabled,
QCheckBox::indicator:indeterminate:disabled {
	image: url("img/checkbox-mixed-insensitive.svg");
}

/* --- radio --- */

QRadioButton::indicator {
	width: 16px;
	height: 16px;
	margin: 1px 5px 0 0;
	image: url("img/radio-unchecked.svg");
}

QRadioButton::indicator:disabled {
	image: url("img/radio-unchecked-insensitive.svg");
}

QRadioButton::indicator:checked {
	image: url("img/radio-checked.svg");
}

QTreeView QRadioButton::indicator:enabled:checked:selected,
QRadioButton::indicator:checked:pressed {
	image: url("img/radio-checked-active.svg");
}

QRadioButton::indicator:checked:disabled {
	image: url("img/radio-checked-insensitive.svg");
}

QRadioButton::indicator:indeterminate {
	image: url("img/radio-mixed.svg");
}

QTreeView QRadioButton::indicator:enabled:indeterminate:selected,
QRadioButton::indicator:indeterminate:pressed {
	image: url("img/radio-mixed-active.svg");
}

QRadioButton::indicator:indeterminate:disabled {
	image: url("img/radio-mixed-insensitive.svg");
}

/* --- progress bar & slider --- */

QProgressBar {
	text-align: center;
	/* border: 1px solid #B4A292; /* @slider_trough_border */
	selection-color: yellow; /* @theme_fg - not wotking due to ::chunk and border-radius and text-align */
	selection-background-color: blue; /* @theme_selected_fg */
	border-radius: 2px;
	background-color: #D1C2B6; /* @slider_trough_bg */
}

QProgressBar:vertical {
	width: 23px;
}

QProgressBar[textVisible="true"]:vertical {
	width: 40px;
}

QProgressBar:horizontal {
	height: 23px;
}

QProgressBar::chunk {
	border: 1px solid #694C3F; /* @slider_border */
	border-radius: 2px;
}

QSlider {
	padding: 3px;
}

QSlider[hasTicks="TicksAbove"] {
	padding: 3px 1px 3px 8px;
}

QSlider[hasTicks="TicksBelow"] {
	padding: 3px 9px 3px 0; /* @todo 1/8,9/0 */
}

QSlider[hasTicks="TicksBothSides"] {
	padding: 3px 5px;
}

QSlider::groove {
	border: 1px solid #B4A292; /* @slider_trough_border */
	border-radius: 2px;
}

QSlider::handle {
	border: 1px solid #B4A292; /* @slider_trough_border */
	border-radius: 2px;
}

QSlider::add-page,
QSlider::sub-page {
	border-radius: 2px;
}

QSlider::groove:vertical {
	width: 3px;
	background-color: #D1C2B6; /* @slider_trough_bg */
}

QSlider::groove:horizontal {
	height: 3px;
	background-color: #D1C2B6; /* @slider_trough_bg */
}

QSlider::handle:vertical {
	height: 18px;
	margin: 0 -5px;
	background: qlineargradient(x1:0, y1:0, x2:1, y2:0, /* horizontal */
		stop:0       /* @scale_glaze_top */ #EFE8E3,
		stop:0.49   /* @scale_glaze_mida */ #EBE4DD,
		stop:0.4901 /* @scale_glaze_midb */ #DDD2C8,
		stop:1       /* @scale_glaze_bot */ #D8CBC0);
}

QSlider::handle:horizontal {
	width: 18px;
	margin: -5px 0;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		stop:0       /* @scale_glaze_top */ #EFE8E3,
		stop:0.49   /* @scale_glaze_mida */ #EBE4DD,
		stop:0.4901 /* @scale_glaze_midb */ #DDD2C8,
		stop:1       /* @scale_glaze_bot */ #D8CBC0);
}

QProgressBar::chunk:vertical,
QSlider::sub-page:vertical,
QSlider::add-page:vertical[isInverted="true"] {
	background: qlineargradient(x1:0, y1:0, x2:1, y2:0, /* horizontal */
		stop:0       /* @slider_glaze_top */ rgba(150,98,76,0.80),
		stop:0.49   /* @slider_glaze_mida */ rgba(141,94,74,0.80),
		stop:0.4901 /* @slider_glaze_midb */ rgba(123,85,69,0.80),
		stop:1       /* @slider_glaze_bot */ rgba(114,81,66,0.80));
}

QProgressBar::chunk:horizontal,
QSlider::sub-page:horizontal,
QSlider::add-page:horizontal[isInverted="true"] {
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		stop:0       /* @slider_glaze_top */ rgba(150,98,76,0.80),
		stop:0.49   /* @slider_glaze_mida */ rgba(141,94,74,0.80),
		stop:0.4901 /* @slider_glaze_midb */ rgba(123,85,69,0.80),
		stop:1       /* @slider_glaze_bot */ rgba(114,81,66,0.80));
}

QSlider::sub-page[isInverted="true"] {
	background: none;
}

/* --- notebook & tabs --- */

QTabBar {
	/* background-color: #E6DDD5; /* @theme_bg */
}

QTabBar QToolButton {
	max-width: 16px;
	max-height: 16px;
	margin: 0;
	border: 1px solid transparent;
	border-radius: 3px;
}

QTabBar::tab {
	border: 1px solid #C4B3A5; /* @nb_tab_border */
}

QTabBar::tab:top {
	margin: 1px 0 0 0;
	padding: 2px 4px 4px;
	border-bottom-color: #B4A191; /* @nb_border */
	/* border-radius: 2.4px 2.4px 0 0; @todo not working */
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		stop: 0       /* @nb_tab_glaze_top */ #E3DAD1,
		stop: 0.5100 /* @nb_tab_glaze_mida */ #E3D9D0,
		stop: 0.5101 /* @nb_tab_glaze_midb */ #DACEC3,
		stop: 1       /* @nb_tab_glaze_bot */ #D6C9BD);
}

QTabBar::tab:top:selected {
	margin: 0;
	padding: 3px 4px 4px;
	border-color: #B4A191; /* @nb_border */
	border-bottom-color: transparent;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		stop: 0       /* @nb_tab_checked_glaze_top */ #F4EFEB,
		stop: 0.5100 /* @nb_tab_checked_glaze_mida */ #F2EDE9,
		stop: 0.5101 /* @nb_tab_checked_glaze_midb */ #EAE2DB,
		stop: 1       /* @nb_tab_checked_glaze_bot */ #EEE8E2);
}

QTabBar::tab:bottom {
	margin: 0 0 1px 0;
	padding: 2px 4px 4px;
	border-top-color: #B4A191; /* @nb_border */
	/* border-radius: 0 0 2.4px 2.4px; @todo not working */
	background: qlineargradient(x1:0, y1:1, x2:0, y2:0,
		stop: 0       /* @nb_tab_glaze_top */ #E3DAD1,
		stop: 0.5100 /* @nb_tab_glaze_mida */ #E3D9D0,
		stop: 0.5101 /* @nb_tab_glaze_midb */ #DACEC3,
		stop: 1       /* @nb_tab_glaze_bot */ #D6C9BD);
}

QTabBar::tab:bottom:selected {
	margin: 0;
	padding: 2px 4px 5px;
	border-color: #B4A191; /* @nb_border */
	border-top-color: transparent;
	background: qlineargradient(x1:0, y1:1, x2:0, y2:0,
		stop: 0       /* @nb_tab_checked_glaze_top */ #F4EFEB,
		stop: 0.5100 /* @nb_tab_checked_glaze_mida */ #F2EDE9,
		stop: 0.5101 /* @nb_tab_checked_glaze_midb */ #EAE2DB,
		stop: 1       /* @nb_tab_checked_glaze_bot */ #EEE8E2);
}

QTabBar::tab:left {
	margin: 0 0 0 1px;
	padding: 2px 4px 1px;
	border-right-color: #B4A191; /* @nb_border */
	/* border-radius: 2.4px 0 0 2.4px; @todo not working */
	background: qlineargradient(x1:0, y1:0, x2:1, y2:0, /* horizontal */
		stop: 0       /* @nb_tab_glaze_top */ #E3DAD1,
		stop: 0.5000 /* @nb_tab_glaze_mida */ #E3D9D0,
		stop: 0.5001 /* @nb_tab_glaze_midb */ #DACEC3,
		stop: 1       /* @nb_tab_glaze_bot */ #D6C9BD);
}

QTabBar::tab:left:selected {
	margin: 0;
	padding: 3px 4px 1px;
	border-color: #B4A191; /* @nb_border */
	border-right-color: transparent;
	background: qlineargradient(x1:0, y1:0, x2:1, y2:0, /* horizontal */
		stop: 0       /* @nb_tab_checked_glaze_top */ #F4EFEB,
		stop: 0.5000 /* @nb_tab_checked_glaze_mida */ #F2EDE9,
		stop: 0.5001 /* @nb_tab_checked_glaze_midb */ #EAE2DB,
		stop: 1       /* @nb_tab_checked_glaze_bot */ #EEE8E2);
}

QTabBar::tab:right {
	margin: 0 1px 0 0;
	padding: 2px 4px 1px;
	border-left-color: #B4A191; /* @nb_border */
	/* border-radius: 0 2.4px 2.4px 0; @todo not working */
	background: qlineargradient(x1:1, y1:0, x2:0, y2:0,
		stop: 0       /* @nb_tab_glaze_top */ #E3DAD1,
		stop: 0.5000 /* @nb_tab_glaze_mida */ #E3D9D0,
		stop: 0.5001 /* @nb_tab_glaze_midb */ #DACEC3,
		stop: 1       /* @nb_tab_glaze_bot */ #D6C9BD);
}

QTabBar::tab:right:selected {
	margin: 0;
	padding: 3px 4px 1px;
	border-color: #B4A191; /* @nb_border */
	border-left-color: transparent;
	background: qlineargradient(x1:1, y1:0, x2:0, y2:0,
		stop: 0       /* @nb_tab_checked_glaze_top */ #F4EFEB,
		stop: 0.5000 /* @nb_tab_checked_glaze_mida */ #F2EDE9,
		stop: 0.5001 /* @nb_tab_checked_glaze_midb */ #EAE2DB,
		stop: 1       /* @nb_tab_checked_glaze_bot */ #EEE8E2);
}

QTabWidget::pane {
	border: 1px solid #B4A191; /* @nb_border */
}

QTabWidget::pane:top {
	margin-top: -1px;
	/* border-radius: 0 0 2.4px 2.4px; @todo not working */
}

QTabWidget::pane:bottom {
	margin-bottom: -1px;
	/* border-radius: 2.4px 2.4px 0 0; @todo not working */
}

QTabWidget::pane:left {
	margin-left: -1px;
	/* border-radius: 0 2.4px 2.4px 0; @todo not working */
}

QTabWidget::pane:right {
	margin-right: -1px;
	/* border-radius: 2.4px 0 0 2.4px; @todo not working */
}

QTabWidget > QStackedWidget > QWidget {
	background-color: #EEE8E2; /* @nb_bg */
}

/* --- notebook tabs overlap --- */

QTabBar::tab:top:!only-one {
	margin: 1px -1px 0 0;
}

QTabBar::tab:top:!only-one:last {
	margin: 1px 0 0 0;
}

QTabBar::tab:top:!only-one:selected {
	margin: 0 -1px 0 0;
}

QTabBar::tab:bottom:!only-one {
	margin: 0 -1px 1px 0;
}

QTabBar::tab:bottom:!only-one:last {
	margin: 0 0 1px 0;
}

QTabBar::tab:bottom:!only-one:selected {
	margin: 0 -1px 0 0;
}

QTabBar::tab:left:!only-one {
	margin: 0 0 -1px 1px;
}

QTabBar::tab:left:!only-one:last {
	margin: 0 0 0 1px;
}

QTabBar::tab:left:!only-one:selected {
	margin: 0 0 -1px 0;
}

QTabBar::tab:right:!only-one {
	margin: 0 1px -1px 0;
}

QTabBar::tab:right:!only-one:last {
	margin: 0 1px 0 0;
}

QTabBar::tab:right:!only-one:selected {
	margin: 0 0 -1px 0;
}

QTabBar::tab:top:!only-one:last:selected,
QTabBar::tab:bottom:!only-one:last:selected,
QTabBar::tab:left:!only-one:last:selected,
QTabBar::tab:right:!only-one:last:selected {
	margin: 0;
}

/* --- treeview --- */

QHeaderView {
	border: 0;
	border-bottom: 1px solid #C9B9AB; /* @toolbar_border_bot */
	background:
		#E6DDD5 /* @theme_bg */ /* @todo button_glaze_top below */
		qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
			stop:0               /* @theme_bg */ #E6DDD5,
			stop:0.49   /* @button_glaze_mida */ rgba(235,228,221,0.80),
			stop:0.4901 /* @button_glaze_midb */ rgba(218,205,194,0.90),
			stop:1       /* @button_glaze_bot */ rgba(221,210,200,0.90));
}

QHeaderView::section {
	margin: 2px 0;
	padding: 0 3px;
	border: 0;
	border-left: 1px solid rgba(255,255,255,0.50); /* @toolbar_separator_lite */
	border-right: 1px solid rgba(179,161,144,0.50); /* @toolbar_separator_dark */
	background: transparent;
}

QHeaderView::section:first {
	border-left: 0;
}

QHeaderView::section:last {
	border-right: 0;
}

QHeaderView::section:only-one {
	border-left: 0;
	border-right: 0;
}

QHeaderView::down-arrow {
	width: 16px;
	height: 16px;
	margin: 1px 3px 0;
	image: url('/usr/share/icons/Adwaita/symbolic/ui/pan-up-symbolic.svg');
}

QHeaderView::up-arrow {
	width: 16px;
	height: 16px;
	margin: 1px 3px 0;
	image: url('/usr/share/icons/Adwaita/symbolic/ui/pan-down-symbolic.svg');
