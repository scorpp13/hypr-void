QTreeView {
	border: 1px solid #AD9A89; /* @slider_border_treeview */
	background-color: #FFFFFF; /* @theme_base */
	selection-color: #FFFFFF; /* @theme_selected_fg */
	selection-background-color: #8F5F4A; /* @theme_selected_bg */
	alternate-background-color: #EEE; /* @todo */
}

QTreeView::item {
	padding: 2px 1px 3px;
}

QTreeView::item:!only-one {
	background-image: url("img/treeview-separator.svg");
	background-position: right top;
	background-repeat: repeat-y;
}

QTreeView::item:!only-one:last {
	background: transparent;
}

QTreeView::item:selected {
	color: #FFFFFF; /* @theme_selected_fg */
	background-color: #8F5F4A; /* @theme_selected_bg */
}

QTreeView::indicator {
	margin: -1px 0 0 0;
}

QTreeView QRadioButton {
	margin: -1px 0 0 4px;
}

QTreeView QProgressBar {
	margin: 1px 2px;
	border: 1px solid #AD9A89; /* @slider_border_treeview */
	border-radius: 0;
	background-color: #E6DDD5; /* @theme_bg */
}

QTreeView QProgressBar::chunk {
	margin: -1px 0;
	border-radius: 0;
}

/* --- scroll bar --- */

QScrollBar:horizontal {
	height: 21px;
	min-height: 17px;
	margin: 0 15px;
	padding: 5px 0;
	border-top: 1px solid #AD9A89; /* @slider_border_treeview */
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		/* bg color */
		stop:0      /* @theme_bg */ #E6DDD5,
		stop:0.14   /* @theme_bg */ #E6DDD5,
		/* border color */
		stop:0.1401 /* @scroll_border */ #C5B5A6,
		stop:0.1701 /* @scroll_border */ #C5B5A6,
		/* bg color */
		stop:0.1701 /* @scroll_bg */ #EAE3DC,
		stop:1      /* @scroll_bg */ #EAE3DC);
}

QScrollBar::handle:horizontal {
	min-width: 10px;
	margin: 0 0 7px;
	border: 1px solid #9E8B79; /* @scroll_slider_border */
	border-radius: 2px;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		stop:0       /* @scroll_glaze_top */ #C6B7A8,
		stop:0.49   /* @scroll_glaze_mida */ #CEC0B3,
		stop:0.4901 /* @scroll_glaze_midb */ #BEAC9D,
		stop:1       /* @scroll_glaze_bot */ #B9A797);
}

QScrollBar::sub-line:horizontal {
	width: 15px;
	border-top: 1px solid #AD9A89; /* @slider_border_treeview */
	subcontrol-position: left;
	subcontrol-origin: margin;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		/* bg color */
		stop:0      /* @theme_bg */ #E6DDD5,
		stop:0.14   /* @theme_bg */ #E6DDD5,
		/* border color */
		stop:0.1401 /* @scroll_border */ #C5B5A6,
		stop:0.1701 /* @scroll_border */ #C5B5A6,
		/* bg color */
		stop:0.1701 /* @scroll_bg */ #EAE3DC,
		stop:1      /* @scroll_bg */ #EAE3DC);
}

QScrollBar::add-line:horizontal {
	width: 15px;
	border-top: 1px solid #AD9A89; /* @slider_border_treeview */
	subcontrol-position: right;
	subcontrol-origin: margin;
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1, /* vertical */
		/* bg color */
		stop:0      /* @theme_bg */ #E6DDD5,
		stop:0.14   /* @theme_bg */ #E6DDD5,
		/* border color */
		stop:0.1401 /* @scroll_border */ #C5B5A6,
		stop:0.1701 /* @scroll_border */ #C5B5A6,
		/* bg color */
		stop:0.1701 /* @scroll_bg */ #EAE3DC,
		stop:1      /* @scroll_bg */ #EAE3DC);
}

QScrollBar:vertical {
	width: 21px;
	min-width: 17px;
	margin: 15px 0;
	padding: 0 5px;
	border-left: 1px solid #AD9A89; /* @slider_border_treeview */
	background: qlineargradient(x1:0, y1:0, x2:1, y2:0, /* horizontal */
		/* bg color */
		stop:0      /* @theme_bg */ #E6DDD5,
		stop:0.14   /* @theme_bg */ #E6DDD5,
		/* border color */
		stop:0.1401 /* @scroll_border */ #C5B5A6,
		stop:0.1701 /* @scroll_border */ #C5B5A6,
		/* bg color */
		stop:0.1701 /* @scroll_bg */ #EAE3DC,
		stop:1      /* @scroll_bg */ #EAE3DC);
}

QScrollBar::handle:vertical {
	min-height: 10px;
	margin: 0 7px 0 0;
	border: 1px solid #9E8B79; /* @scroll_slider_border */
	border-radius: 2px;
	background: qlineargradient(x1:0, y1:0, x2:1, y2:0, /* horizontal */
		stop:0       /* @scroll_glaze_top */ #C6B7A8,
		stop:0.49   /* @scroll_glaze_mida */ #CEC0B3,
		stop:0.4901 /* @scroll_glaze_midb */ #BEAC9D,
		stop:1       /* @scroll_glaze_bot */ #B9A797);
}

QScrollBar::sub-line:vertical {
	height: 15px;
	border-left: 1px solid #AD9A89; /* @slider_border_treeview */
	subcontrol-position: top;
	subcontrol-origin: margin;
	background: qlineargradient(x1:0, y1:0, x2:1, y2:0, /* horizontal */
		/* bg color */
		stop:0      /* @theme_bg */ #E6DDD5,
